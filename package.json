    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.4",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "microbundle": "^0.8.4",
    "netlify-cli": "^2.11.23",
